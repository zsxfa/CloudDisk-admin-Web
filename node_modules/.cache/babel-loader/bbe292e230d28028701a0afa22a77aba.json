;AASA,MAAA,uBAAA,EAAA,KATA;AAUA,MAAA,IAAA,EAAA,QAVA;AAWA,MAAA,UAAA,EAAA,OAXA;AAYA,MAAA,iBAAA,EAAA;AACA,QAAA,IAAA,EAAA,CAAA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,CADA;AAEA,QAAA,IAAA,EAAA,CACA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CAFA;AAKA,QAAA,SAAA,EAAA,CACA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AALA,OAZA;AAqBA,MAAA,uBAAA,EAAA;AACA,QAAA,IAAA,EAAA,CACA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CADA;AAIA,QAAA,eAAA,EAAA,CACA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AAJA;AArBA,KAAA;AA8BA,GAhCA;AAiCA;AACA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,GAAA;AACA;AAHA,GAlCA;AAwCA,EAAA,OAxCA,qBAwCA;AACA,SAAA,aAAA;AACA,GA1CA;AA4CA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AAAA;;AACA;AACA,MAAA,IAAA,CAAA,iBAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,OAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,QAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OALA;AAMA,KATA;AAUA,IAAA,UAVA,sBAUA,KAVA,EAUA,IAVA,EAUA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,OAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA;AACA,KAdA;AAeA,IAAA,MAfA,kBAeA,IAfA,EAeA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAEA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,eAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA,OAPA,EAQA,IARA,CAQA,YAAA;AACA,QAAA,MAAA,CAAA,aAAA,GADA,CACA;;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAdA,EAeA,KAfA,CAeA,UAAA,QAAA,EAAA;AACA;AACA,YAAA,QAAA,KAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA;AACA,OA5BA;AA6BA,KA/CA;AAgDA,IAAA,gBAhDA,8BAgDA;AAAA;;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,EAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,UAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,YAAA,CAAA,MAAA,CAAA,UAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,uBAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,IAAA,EAAA,SADA;AAEA,gBAAA,OAAA,EAAA;AAFA,eAAA,EAFA,CAMA;;;AACA,cAAA,MAAA,CAAA,aAAA;;AACA,cAAA,MAAA,CAAA,IAAA,qBAAA,QAAA;AACA,cAAA,MAAA,CAAA,UAAA,qBAAA,OAAA;AACA,aAVA;AAWA;AACA;AACA,OAlBA;AAmBA,KApEA;AAqEA,IAAA,cArEA,4BAqEA;AAAA;;AACA,MAAA,IAAA,CACA,YADA,CACA,KAAA,IADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAFA,CAMA;;;AACA,QAAA,MAAA,CAAA,aAAA;;AACA,QAAA,MAAA,CAAA,IAAA,qBAAA,QAAA;AACA,QAAA,MAAA,CAAA,UAAA,qBAAA,OAAA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,QAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,IAAA,qBAAA,QAAA;AACA,QAAA,MAAA,CAAA,UAAA,qBAAA,OAAA;AACA,OAtBA;AAuBA,KA7FA;AA+FA,IAAA,MA/FA,oBA+FA;AAAA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA;;AACA,cAAA,CAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA;AACA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,GAAA,IAAA,CAAA,EAAA;AACA,cAAA,MAAA,CAAA,cAAA,GADA,CACA;;AACA,aAFA,MAEA;AACA,cAAA,MAAA,CAAA,cAAA,GADA,CACA;;AACA;AACA,WAPA,MAOA;AACA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA;AACA;AACA,OAfA;AAgBA,KAhHA;AAkHA,IAAA,MAlHA,kBAkHA,GAlHA,EAkHA;AAAA;;AACA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAFA,CAMA;;;AACA,QAAA,MAAA,CAAA,aAAA;;AACA,QAAA,MAAA,CAAA,QAAA;AACA,OATA;AAUA,KA9HA;AA+HA,IAAA,cA/HA,4BA+HA;AAAA;;AACA,MAAA,IAAA,CACA,YADA,CACA,KAAA,IADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA,CAFA,CAGA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAJA,CAQA;;;AACA,QAAA,MAAA,CAAA,aAAA,GATA,CAUA;;;AACA,QAAA,MAAA,CAAA,IAAA,qBAAA,QAAA;AACA,QAAA,MAAA,CAAA,UAAA,qBAAA,OAAA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,QAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA,CAFA,CAGA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAJA,CAQA;;;AACA,QAAA,MAAA,CAAA,IAAA,qBAAA,QAAA;AACA,QAAA,MAAA,CAAA,UAAA,qBAAA,OAAA;AACA,OA3BA;AA4BA,KA5JA;AA6JA,IAAA,OA7JA,mBA6JA,IA7JA,EA6JA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAhKA;AAiKA,IAAA,cAjKA,0BAiKA,IAjKA,EAiKA;AACA,WAAA,uBAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KApKA;AAqKA,IAAA,QArKA,sBAqKA;AACA,WAAA,uBAAA,GAAA,KAAA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,IAAA,qBAAA,QAAA;AACA,WAAA,UAAA,qBAAA,OAAA;AACA;AA1KA;AA5CA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <!-- <el-input\r\n      v-model=\"filterText\"\r\n      placeholder=\"Filter keyword\"\r\n      style=\"margin-bottom: 30px\"\r\n    /> -->\r\n\r\n    <el-table\r\n      :data=\"menuList\"\r\n      style=\"width: 100%; margin-bottom: 20px\"\r\n      row-key=\"id\"\r\n      border\r\n      default-expand-all\r\n      :tree-props=\"{ children: 'children', hasChildren: 'hasChildren' }\"\r\n    >\r\n      <el-table-column\r\n        prop=\"name\"\r\n        label=\"名称\"\r\n        sortable\r\n        width=\"180\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"path\"\r\n        label=\"访问路径\"\r\n        sortable\r\n        width=\"180\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"component\"\r\n        label=\"组件路径\"\r\n        sortable\r\n        width=\"180\"\r\n      ></el-table-column>\r\n      <el-table-column prop=\"permissionValue\" label=\"权限值\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <!-- v-if=\"node.level == 1 || node.level == 2\" v-if=\"node.level == 3\" v-if=\"node.level == 4\"-->\r\n          <el-button\r\n            v-if=\"\r\n              (scope.row.level == 1 || scope.row.level == 2) &&\r\n              hasPerm('permission.add')\r\n            \"\r\n            type=\"text\"\r\n            size=\"mini\"\r\n            @click=\"\r\n              () => {\r\n                ;(dialogFormVisible = true), (menu.pid = scope.row.id)\r\n              }\r\n            \"\r\n          >\r\n            添加菜单\r\n          </el-button>\r\n          <el-button\r\n            v-if=\"scope.row.level == 3 && hasPerm('permission.add')\"\r\n            type=\"text\"\r\n            size=\"mini\"\r\n            @click=\"\r\n              () => {\r\n                ;(dialogPermissionVisible = true),\r\n                  (permission.pid = scope.row.id)\r\n              }\r\n            \"\r\n          >\r\n            添加功能\r\n          </el-button>\r\n          <el-button\r\n            v-if=\"scope.row.level == 4 && hasPerm('permission.update')\"\r\n            type=\"text\"\r\n            size=\"mini\"\r\n            @click=\"() => updateFunction(scope.row)\"\r\n          >\r\n            修改功能\r\n          </el-button>\r\n          <el-button\r\n            v-if=\"scope.row.level != 4 && hasPerm('permission.update')\"\r\n            type=\"text\"\r\n            size=\"mini\"\r\n            @click=\"() => getById(scope.row)\"\r\n          >\r\n            修改\r\n          </el-button>\r\n          <el-button\r\n            type=\"text\"\r\n            size=\"mini\"\r\n            @click=\"() => remove(scope.row)\"\r\n            v-if=\"hasPerm('permission.remove')\"\r\n          >\r\n            删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog :visible.sync=\"dialogFormVisible\" :title=\"dialogFormValue\">\r\n      <el-form\r\n        ref=\"menu\"\r\n        :model=\"menu\"\r\n        :rules=\"menuValidateRules\"\r\n        label-width=\"120px\"\r\n      >\r\n        <el-form-item label=\"菜单名称\" prop=\"name\">\r\n          <el-input v-model=\"menu.name\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"访问路径\" prop=\"path\">\r\n          <el-input v-model=\"menu.path\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"组件路径\" prop=\"component\">\r\n          <el-input v-model=\"menu.component\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"restData()\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"append()\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 添加功能的窗口 -->\r\n    <el-dialog :visible.sync=\"dialogPermissionVisible\" title=\"添加功能\">\r\n      <el-form\r\n        ref=\"permission\"\r\n        :model=\"permission\"\r\n        :rules=\"permissionValidateRules\"\r\n        label-width=\"120px\"\r\n      >\r\n        <el-form-item label=\"功能名称\" prop=\"name\">\r\n          <el-input v-model=\"permission.name\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"访问路径\">\r\n          <el-input v-model=\"permission.path\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"组件路径\">\r\n          <el-input v-model=\"permission.component\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"功能权限值\" prop=\"permissionValue\">\r\n          <el-input v-model=\"permission.permissionValue\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"restData()\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"appendPermission()\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport menu from '@/api/acl/menu'\r\n\r\nconst menuForm = {\r\n  name: '',\r\n  pid: 0,\r\n  path: '',\r\n  component: '',\r\n  type: '1',\r\n}\r\nconst perForm = {\r\n  permissionValue: '',\r\n  type: '2',\r\n  name: '',\r\n  path: '',\r\n  component: '',\r\n  pid: 0,\r\n}\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      filterText: '',\r\n      menuList: [],\r\n      defaultProps: {\r\n        children: 'children',\r\n        label: 'name',\r\n      },\r\n      dialogFormValue: '添加菜单',\r\n      dialogFormVisible: false,\r\n      dialogPermissionVisible: false,\r\n      menu: menuForm,\r\n      permission: perForm,\r\n      menuValidateRules: {\r\n        name: [{ required: true, trigger: 'blur', message: '菜单名必须输入' }],\r\n        path: [\r\n          { required: true, trigger: 'blur', message: '菜单路径必须输入' },\r\n        ],\r\n        component: [\r\n          { required: true, trigger: 'blur', message: '组件名称必须输入' },\r\n        ],\r\n      },\r\n      permissionValidateRules: {\r\n        name: [\r\n          { required: true, trigger: 'blur', message: '功能名称必须输入' },\r\n        ],\r\n        permissionValue: [\r\n          { required: true, trigger: 'blur', message: '功能权限值必须输入' },\r\n        ],\r\n      },\r\n    }\r\n  },\r\n  // 监听上面文本框搜索\r\n  watch: {\r\n    filterText(val) {\r\n      this.$refs.menuTree.filter(val)\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.fetchNodeList()\r\n  },\r\n\r\n  methods: {\r\n    fetchNodeList() {\r\n      // debugger\r\n      menu.getNestedTreeList().then((response) => {\r\n        if (response.success === true) {\r\n          this.menuList = response.data.children\r\n          console.log(this.menuList)\r\n        }\r\n      })\r\n    },\r\n    filterNode(value, data) {\r\n      if (!value) return true\r\n      console.log(data)\r\n      return data.title.toLowerCase().indexOf(value.toLowerCase()) !== -1\r\n    },\r\n    remove(data) {\r\n      console.log(data)\r\n\r\n      this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning',\r\n      })\r\n        .then(() => {\r\n          return menu.removeById(data.id)\r\n        })\r\n        .then(() => {\r\n          this.fetchNodeList() // 刷新列表\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功!',\r\n          })\r\n        })\r\n        .catch((response) => {\r\n          // 失败\r\n          if (response === 'cancel') {\r\n            this.$message({\r\n              type: 'info',\r\n              message: '已取消删除',\r\n            })\r\n          } else {\r\n            this.$message({\r\n              type: 'error',\r\n              message: '删除失败',\r\n            })\r\n          }\r\n        })\r\n    },\r\n    appendPermission() {\r\n      this.$refs.permission.validate((valid) => {\r\n        if (valid) {\r\n          if (this.permission.id) {\r\n            this.update(this.permission)\r\n          } else {\r\n            menu.saveLevelOne(this.permission).then((response) => {\r\n              this.dialogPermissionVisible = false\r\n              this.$message({\r\n                type: 'success',\r\n                message: '添加功能成功',\r\n              })\r\n              // 刷新页面\r\n              this.fetchNodeList()\r\n              this.menu = { ...menuForm }\r\n              this.permission = { ...perForm }\r\n            })\r\n          }\r\n        }\r\n      })\r\n    },\r\n    appendLevelOne() {\r\n      menu\r\n        .saveLevelOne(this.menu)\r\n        .then((response) => {\r\n          this.dialogFormVisible = false\r\n          this.$message({\r\n            type: 'success',\r\n            message: '添加一级菜单成功',\r\n          })\r\n          // 刷新页面\r\n          this.fetchNodeList()\r\n          this.menu = { ...menuForm }\r\n          this.permission = { ...perForm }\r\n        })\r\n        .catch((response) => {\r\n          // 你们写：判断点击取消清空一下\r\n          this.dialogFormVisible = false\r\n          this.$message({\r\n            type: 'error',\r\n            message: '添加一级菜单失败',\r\n          })\r\n          this.menu = { ...menuForm }\r\n          this.permission = { ...perForm }\r\n        })\r\n    },\r\n\r\n    append() {\r\n      this.$refs.menu.validate((valid) => {\r\n        if (valid) {\r\n          debugger\r\n          if (!this.menu.id) {\r\n            // 添加\r\n            if (this.menu.pid == 0) {\r\n              this.appendLevelOne() // 一级分类的添加\r\n            } else {\r\n              this.appendLevelTwo() // 二级分类的添加\r\n            }\r\n          } else {\r\n            // 修改\r\n            this.update(this.menu)\r\n          }\r\n        }\r\n      })\r\n    },\r\n\r\n    update(obj) {\r\n      debugger\r\n      menu.update(obj).then((response) => {\r\n        this.dialogFormVisible = false\r\n        this.$message({\r\n          type: 'success',\r\n          message: '修改成功',\r\n        })\r\n        // 刷新页面\r\n        this.fetchNodeList()\r\n        this.restData()\r\n      })\r\n    },\r\n    appendLevelTwo() {\r\n      menu\r\n        .saveLevelOne(this.menu)\r\n        .then((response) => {\r\n          // 1、把文本框关\r\n          this.dialogFormVisible = false\r\n          // 2、提示成功\r\n          this.$message({\r\n            type: 'success',\r\n            message: '添加二级分类成功',\r\n          })\r\n          // 3、刷新页面\r\n          this.fetchNodeList()\r\n          // 4、把menu清空\r\n          this.menu = { ...menuForm }\r\n          this.permission = { ...perForm }\r\n        })\r\n        .catch((response) => {\r\n          // 1、把文本框关\r\n          this.dialogFormVisible = false\r\n          // 2、提示成功\r\n          this.$message({\r\n            type: 'error',\r\n            message: '添加二级分类失败',\r\n          })\r\n          // 3、把menu清空\r\n          this.menu = { ...menuForm }\r\n          this.permission = { ...perForm }\r\n        })\r\n    },\r\n    getById(data) {\r\n      this.dialogFormVisible = true\r\n      this.menu = data\r\n    },\r\n    updateFunction(data) {\r\n      this.dialogPermissionVisible = true\r\n      this.permission = data\r\n    },\r\n    restData() {\r\n      this.dialogPermissionVisible = false\r\n      this.dialogFormVisible = false\r\n      this.menu = { ...menuForm }\r\n      this.permission = { ...perForm }\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/acl/menu"}]}