AAEbM,MAAAA,MAAM,EAAE,MAFK;AAGbC,MAAAA,MAAM,EAAE;AAAEQ,QAAAA,MAAF;AAAUE,QAAAA;AAAV;AAHK,KAAD,CAAd;AAKD,GA1CY;;AA2CbC,EAAAA,UAAU,CAACT,EAAD,EAAK;AACb,WAAOV,OAAO,CAAC;AACbM,MAAAA,GAAG,EAAG,GAAEL,QAAS,WAAUS,EAAG,EADjB;AAEbH,MAAAA,MAAM,EAAE;AAFK,KAAD,CAAd;AAID,GAhDY;;AAiDba,EAAAA,UAAU,CAACC,MAAD,EAAS;AACjB,WAAOrB,OAAO,CAAC;AACbM,MAAAA,GAAG,EAAG,GAAEL,QAAS,cADJ;AAEbM,MAAAA,MAAM,EAAE,QAFK;AAGbM,MAAAA,IAAI,EAAEQ;AAHO,KAAD,CAAd;AAKD;;AAvDY,CAAf","sourcesContent":["import request from '@/utils/request'\n\nconst api_name = '/admin/acl/user'\n\nexport default {\n  getPageList(page, limit, searchObj) {\n    return request({\n      url: `${api_name}/${page}/${limit}`,\n      method: 'get',\n      params: searchObj, // url查询字符串或表单键值对\n    })\n  },\n  getById(id) {\n    return request({\n      url: `${api_name}/get/${id}`,\n      method: 'get',\n    })\n  },\n\n  save(user) {\n    return request({\n      url: `${api_name}/save`,\n      method: 'post',\n      data: user,\n    })\n  },\n\n  updateById(user) {\n    return request({\n      url: `${api_name}/update`,\n      method: 'put',\n      data: user,\n    })\n  },\n  getAssign(userId) {\n    return request({\n      url: `${api_name}/toAssign/${userId}`,\n      method: 'get',\n    })\n  },\n  saveAssign(userId, roleId) {\n    return request({\n      url: `${api_name}/doAssign`,\n      method: 'post',\n      params: { userId, roleId },\n    })\n  },\n  removeById(id) {\n    return request({\n      url: `${api_name}/remove/${id}`,\n      method: 'delete',\n    })\n  },\n  removeRows(idList) {\n    return request({\n      url: `${api_name}/batchRemove`,\n      method: 'delete',\n      data: idList,\n    })\n  },\n}\n"]}]}